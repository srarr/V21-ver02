# Environment configuration example for Heliox ATLAS V21
# Copy this file to .env and replace the values

# Backend API configuration
BACKEND_HTTP_ADDR=http://localhost:8080

# Supabase Local configuration (get from `supabase status`)
SUPABASE_PROJECT_URL=http://localhost:54321
SUPABASE_ANON_KEY=REPLACE_ME
SUPABASE_SERVICE_KEY=REPLACE_ME

# Database configuration
POSTGRES_DSN=postgres://postgres:postgres@localhost:54322/postgres?sslmode=disable
DATABASE_URL=postgres://postgres:postgres@localhost:54322/postgres

# Development environment
NODE_ENV=development
GO_ENV=development

# API Keys (optional for local development)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Port configuration
API_PORT=8080
WEB_PORT=5173
